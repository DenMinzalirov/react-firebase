  ],
  "keywords": [
    "react",
    "reactjs",
    "firebase"
  ],
  "author": "Simen Brekken <simen@unfold.no> (http://github.com/unfold)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unfold/react-firebase/issues"
  },
  "homepage": "https://github.com/unfold/react-firebase",
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "eslint": "2.4.0",
    "eslint-config-airbnb": "6.1.0",
    "eslint-plugin-react": "4.2.3",
    "express": "4.13.4",
    "express-urlrewrite": "1.2.0",
    "isparta": "4.0.0",
    "jsdom": "8.1.0",
    "jsdom-global": "1.7.0",
    "react-addons-test-utils": "0.14.7",

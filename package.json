  "dependencies": {
    "babel-core": "6.7.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "estraverse-fb": "1.3.1",
    "firebase": "2.4.1",
    "invariant": "^2.0.0",
    "lodash": "^4.2.0",
    "react": "0.14.7"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-rc.1"
